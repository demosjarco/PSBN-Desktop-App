{
	"name": "psbn-desktop-app",
	"version": "1.0.0",
	"main": "main.js",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"less": "less-watch-compiler src\/components src\/components",
		"electron": "electron .",
		"dev": "concurrently --kill-others \"npm run less\" \"SET BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && npm run electron\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^4.1.0",
		"electron": "^11.5.0",
		"less": "^3.9.0",
		"less-watch-compiler": "^1.13.0",
		"wait-on": "^3.2.0"
	},
	"dependencies": {
		"electron-window-state": "^5.0.3",
		"material-design-icons": "^3.0.1",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-scripts": "3.0.1",
		"request": "^2.88.0"
	}
}
